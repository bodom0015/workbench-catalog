{
    "key": "openrefine",
    "label": "OpenRefine",
    "description": "A free, open source, power tool for working with messy data",
    "logo": "https://archive.bigdatauniversity.com/bdu-wp/wp-content/uploads/2015/09/OpenRefine-BDU-logo-150x150.png",
    "image": {
        "registry": "",
        "name": "felixlohmeier/openrefine",
        "tags": [
            "latest"
        ]
    },
    "display": "stack",
    "access": "external",
    "ports": [
        {
            "port": 3333,
            "protocol": "http",
            "contextPath": "/"
        }
    ],
    "readinessProbe": {
        "type": "http",
        "path": "",
        "port": 3333,
        "initialDelay": 20,
        "timeout": 360
    },
    "volumeMounts": [
        {
            "mountPath": "/data"
        }
    ],
    "resourceLimits": {
        "cpuMax": 500,
        "cpuDefault": 100,
        "memMax": 1000,
        "memDefault": 50
    },
    "authRequired": true
}
